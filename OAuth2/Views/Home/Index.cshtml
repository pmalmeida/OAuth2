@using System.Web.Configuration
@{
    ViewBag.Title = "Index";

    //https://developers.google.com/identity/protocols/OAuth2UserAgent#formingtheurl

    var googleModel = new
    {
        EndPoint = WebConfigurationManager.AppSettings["oauth2-google-endpoint"],
        ClientId = WebConfigurationManager.AppSettings["oauth2-google-clientId"],
        Secret = WebConfigurationManager.AppSettings["oauth2-google-secret"]
    };
}
<div ng-controller="googleViewModel">
    <div class="page-header">
        <h1>Example page header <small>Subtext for header</small></h1>
    </div>

    <div class="form-group">
        <label class="sr-only" for="form-group-input">My label</label>
        <input type="text" class="form-control" id="form-group-input" placeholder="My label" ng-model="name">
    </div>

    <div class="jumbotron">
        <h1>Jumbotron heading</h1>
        <p>{{name}}</p>
    </div>

    <button class="btn btn-default" ng-click="login()">login</button>

</div>

@section script{
    <script src="~/scripts/app/app.js"></script>
    <script>
        var oauth2GoogleClientId = '@(googleModel.ClientId)';
    </script>
}